public class Solution {
    public int[] twoSum(int[] nums, int target) {
        HashMap<Integer,Integer> h = new HashMap<Integer,Integer>();
        int len = nums.length;
        int[] res = new int[2];
        for(int i=0; i<len; i++)
        {
            if(h.containsKey(target-nums[i]))
            {
                res[0] = h.get(target-nums[i]);
                res[1] = i;
                return res;
            }
            else
                h.put(nums[i],i);
        }
        int[] r = {};
        return r;
        
    }
}
